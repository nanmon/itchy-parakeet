        <attr name="index" format="enum">
            <enum name="cursor" value="0" />
            <enum name="grandma" value="1" />
            <enum name="farm" value="2" />
            <enum name="factory" value="3" />
            <enum name="mine" value="4" />
            <enum name="shipment" value="5" />
            <enum name="alchemy_lab" value="6" />
